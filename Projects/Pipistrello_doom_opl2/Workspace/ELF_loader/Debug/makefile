-include ../makefile.init

RM := rm -rf

# All of the sources participating in the build are defined here
-include sources.mk
-include src/subdir.mk
-include subdir.mk
-include objects.mk

ifneq ($(MAKECMDGOALS),clean)
ifneq ($(strip $(C_DEPS)),)
-include $(C_DEPS)
endif
ifneq ($(strip $(S_UPPER_DEPS)),)
-include $(S_UPPER_DEPS)
endif
endif

-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
ELFSIZE += \
ELF_loader.elf.size \

ELFCHECK += \
ELF_loader.elf.elfcheck \


# All Target
all: ELF_loader.elf secondary-outputs

# Tool invocations
ELF_loader.elf: $(OBJS) ../src/lscript.ld $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: MicroBlaze gcc linker'
	mb-gcc -Wl,-T -Wl,../src/lscript.ld -L../../standalone_bsp/microblaze_0/lib -mlittle-endian -mxl-barrel-shift -mxl-pattern-compare -mcpu=v8.40.b -mno-xl-soft-mul -Wl,--no-relax -o "ELF_loader.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

ELF_loader.elf.size: ELF_loader.elf
	@echo 'Invoking: MicroBlaze Print Size'
	mb-size ELF_loader.elf  |tee "ELF_loader.elf.size"
	@echo 'Finished building: $@'
	@echo ' '

ELF_loader.elf.elfcheck: ELF_loader.elf
	@echo 'Invoking: Xilinx ELF Check'
	elfcheck ELF_loader.elf -hw ../../mb_system_hw_platform/system.xml -pe microblaze_0  |tee "ELF_loader.elf.elfcheck"
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) $(OBJS)$(C_DEPS)$(EXECUTABLES)$(ELFSIZE)$(ELFCHECK)$(S_UPPER_DEPS) ELF_loader.elf
	-@echo ' '

secondary-outputs: $(ELFSIZE) $(ELFCHECK)

.PHONY: all clean dependents
.SECONDARY:

-include ../makefile.targets
